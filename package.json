{
	"name": "qwik-trpc-starter",
	"description": "qwik-trpc-starter",
	"engines": {
		"node": ">=15.0.0"
	},
	"private": true,
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.types": "tsc --incremental --noEmit",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint src/**/*.ts*",
		"preview": "qwik build preview && vite preview --open",
		"serve": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
		"start": "vite --open --mode ssr",
		"qwik": "qwik",
		"db-seed": "prisma db seed",
		"prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
		"migrate-sqlite": "prisma migrate dev --name init --schema=./prisma/schema.prisma",
		"generate-sqlite": "prisma generate --schema=./prisma/schema.prisma",
		"qwik-trpc:dev": "run-s migrate-sqlite generate-sqlite db-seed && npm run dev"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"devDependencies": {
		"@builder.io/qwik": "0.12.1",
		"@builder.io/qwik-city": "0.0.118",
		"@types/eslint": "8.4.8",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "5.41.0",
		"@typescript-eslint/parser": "5.41.0",
		"eslint": "8.26.0",
		"eslint-plugin-qwik": "0.12.1",
		"node-fetch": "3.2.10",
		"npm-run-all": "^4.1.5",
		"prettier": "2.7.1",
		"tsx": "^3.11.0",
		"typescript": "4.8.4",
		"vite": "3.2.1",
		"vite-tsconfig-paths": "3.5.0"
	},
	"dependencies": {
		"@prisma/client": "^4.5.0",
		"@trpc/client": "^10.0.0-rc.3",
		"@trpc/server": "^10.0.0-rc.3",
		"prisma": "^4.5.0",
		"superjson": "^1.11.0",
		"zod": "^3.19.1"
	}
}
